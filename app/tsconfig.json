{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": true,
    "removeComments": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "allowJs": true,
    "paths": {
      "~/": ["./src/nuxt"],
      "~/assets/*": ["./src/nuxt/assets/*"],
      "~/components/*": ["./src/nuxt/components/*"],
      "~/middleware/*": ["./src/nuxt/middleware/*"],
      "~/pages/*": ["./src/nuxt/pages/*"],
      "~/plugins/*": ["./src/nuxt/plugins/*"],
      "~/static/*": ["./src/nuxt/static/*"],
      "~/store/*": ["./src/nuxt/store/*"],
    }
  }
}
